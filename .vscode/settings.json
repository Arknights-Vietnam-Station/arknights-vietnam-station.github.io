{
    "[typescript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "files.associations": {
        "*.css": "tailwindcss"
    },
    "files.exclude": {
        ".next": true,
        "package-lock.json": true,
        "node_modules": true,
    },
    "eslint.format.enable": true,
    "editor.quickSuggestions": {
        "strings": "on"
    },
}